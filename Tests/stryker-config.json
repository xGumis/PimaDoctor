{
  "stryker-config":
  {
    "reporters": [
      "progress",
      "html"
    ],
    "log-level": "info",
    "log-file":true,
    "timeout-ms": 10000,
    "project-file": "PimaDoctor.csproj",
    "solution-path": "../PimaDoctor.sln",
    "mutation-level": "Standard",
    "threshold-high": 80,
    "threshold-low": 70,
    "threshold-break": 60,
    "ignore-methods": [
      "*Log*",
      "ToString",
      "*HashCode*"
    ],
    "mutate": [
      "!**/*PythonExtensions.cs",
      "!**/*AbstractControlDescriptionProvider.cs",
      "!**/*Views/*.cs"
    ],
    "test-projects": [
      "Tests.csproj"
    ]
  }
}